{
  "version": "0.2",
  "language": "en",
  "ignorePaths": [
    ".cspell.json",
    "build",
    "node_modules",
    "**/*.tgz",
    "**/package.json",
    "**/tsconfig.json",
    ".vscode/**"
  ],
  "ignoreRegExpList": [
    "0x[A-Z]{7,25}",
    "\\S{32,}",
    "[A-Za-z0-9+/]+={1,2}",
    "deadbe[ea]f",
    "'[a-z0-9]{16}'"
  ],
  "words": [
    "bech",
    "cardano",
    "cbor",
    "Deleg",
    "deregistration",
    "DREP",
    "fixability",
    "ipfs",
    "multiasset",
    "multiassets",
    "reserialized",
    "unfixable",
    "vacuumlabs"
  ]
}
